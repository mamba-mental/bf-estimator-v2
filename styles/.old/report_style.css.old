/* report_style.css */
body {
    font-family: 'Times New Roman', Georgia, serif;
    margin: 20px;
}

h1, h2, h3, h4, h5, h6 {
    font-family: 'Georgia', serif;
    color: #333333;
}

table {
    width: 100%;
    border-collapse: collapse;
    margin-bottom: 20px;
}

table, th, td {
    border: 1px solid #333333;
    padding: 10px;
}

th {
    background-color: #f2f2f2;
    text-align: center;
}

td {
    text-align: left;
}

footer {
    font-size: 12px;
    text-align: center;
    color: #666666;
    margin-top: 50px;
}